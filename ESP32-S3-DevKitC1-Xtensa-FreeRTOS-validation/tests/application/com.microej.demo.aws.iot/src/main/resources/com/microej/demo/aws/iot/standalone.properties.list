# Declare the network interface names
# On simulation use a known interface connected to the Internet
mobile.interface.name=mo0
eth.interface.name=st1
wifi.interface.name=st1

# Select the network interface to be used for AWS communication.
com.microej.demo.aws.iot.NetworkManager=com.microej.demo.aws.iot.EthernetNetworkManager
#com.microej.demo.aws.iot.NetworkManager=com.microej.demo.aws.iot.MobileNetworkManager
#com.microej.demo.aws.iot.NetworkManager=com.microej.demo.aws.iot.WifiNetworkManager

# Access Point Name
mobile.apn=net

# User credential for APN connection
mobile.user=

# Passphrase credential for APN connection
mobile.passphrase=

# PIN code to unlock the SIM card
mobile.pin.code=0000

# Wi-Fi SSID
wifi.ssid=TEST_SSID

# Passphrase credential for Wi-Fi connection
wifi.passphrase=TEST_PASSPHRASE

# Security mode for Wi-Fi connection (MIXED (WPA1 / WPA2)=6; OPEN=7; WPA1=9; WPA2=10; WPA3=11; MIXED (WPA2 / WPA3)=12)
wifi.security=10

# Host of the AWS broker
aws.broker.host=a31i5tkhbu6syw-ats.iot.us-west-2.amazonaws.com

# Port of the AWS broker
aws.broker.port=8883

# Identifier of the Thing
aws.thing.id=MicroEJ_Demo