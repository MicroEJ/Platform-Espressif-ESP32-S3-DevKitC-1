# Declare the network interface names
# On simulation use a known interface connected to the Internet
#mobile.interface.name=mo0
#eth.interface.name=st1
wifi.interface.name=st1
#network.interface=eth0,wlan1,mo2

# Select the network interfaces to be used for HTTP communication.
#net.http.download.manager.IEthernetNetworkManager=net.http.download.manager.EthernetNetworkManager
#net.http.download.manager.IMobileNetworkManager=net.http.download.manager.MobileNetworkManager
net.http.download.manager.IWifiNetworkManager=net.http.download.manager.WifiNetworkManager

# Access Point Name
mobile.apn=net

# User credential for APN connection
mobile.user=

# Passphrase credential for APN connection
mobile.passphrase=

# PIN code to unlock the SIM card
mobile.pin.code=0000

# Wi-Fi SSID
wifi.ssid=MY_SSID

# Passphrase credential for Wi-Fi connection
wifi.passphrase=MY_PWD

# Security mode for Wi-Fi connection (MIXED (WPA1 / WPA2)=6; OPEN=7; WPA1=9; WPA2=10; WPA3=11; MIXED (WPA2 / WPA3)=12)
wifi.security=10

# Number of download threads per network interface (each thread uses it's own http connection)
download.thread.num=3

# Number of download loops each thread performs
download.thread.loop=5

# URL of the resource to download
download.resource.url=http://www.cmps.mai.gov.ro/old/img/MAE_CMPS/Untitled_Panorama1.jpg

# CRC32 of the resource to download
# download.resource.crc32=f769e394
download.resource.crc32=6d4b36c6

# Log once every X downloads at INFO level, otherwise FINE level is used.
# Defaults to 1: always log at INFO level.
log.filter=1
